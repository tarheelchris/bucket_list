<%= form_for(@item, :url => { :action => "create" }, :html => {:multipart => true}) do |f| %>


<div class = "signuptitle">
<%= @current_user.lists.find(params[:list_id]).title %> Bucket List
</div>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
 
 	<!-- <div class="field">
    <%= f.label :details %><br />
    <%= f.text_area :details %>
  </div> -->

  <p>
    <%= f.file_field :image %>
  </p>
  
  <div class="field">
 
     <%= f.hidden_field :complete_status, :value => false %>
   </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
